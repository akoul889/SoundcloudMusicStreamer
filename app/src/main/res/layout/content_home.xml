<?xml version="1.0" encoding="utf-8"?>
<com.quintype.musicstreaming.ui.slidinguppanel.SlidingUpPanelLayout
    android:id="@+id/sliding_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:fadeColor="@android:color/transparent"
    sothree:layout_behavior="@string/appbar_scrolling_view_behavior"
    sothree:panelHeight="55dp"
    sothree:shadowHeight="5dp"
    tools:context=".ui.activities.MainActivity"
    tools:showIn="@layout/app_bar_home"

    >


    <android.widget.FrameLayout
        android:id="@+id/home_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <FrameLayout
        android:id="@+id/quickcontrols_container"
        android:layout_width="match_parent"
        android:layout_height="250dp">


    </FrameLayout>

</com.quintype.musicstreaming.ui.slidinguppanel.SlidingUpPanelLayout>


